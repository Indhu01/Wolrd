<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.i.dao.model" table="supplier">
<id name="supplier_No" type="java.lang.Integer">
<generator class="increment"></generator>
</id>
<property name="supplier_ID" type="java.lang.String"></property>
<property name="region" type="java.lang.String"></property>
<property name="supplier_Name" type="java.lang.String"></property>
<property name="Supplier_Email" type="java.lang.String"></property>

</class>


</hibernate-mapping>